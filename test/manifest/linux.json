{
    "apiVersion": "vlabs",
    "location": "westus2",
    "properties": {
        "orchestratorProfile": {
            "orchestratorType": "Kubernetes",
            "orchestratorRelease": "1.16",
            "kubernetesConfig": {
                "useManagedIdentity": false,
                "useCloudControllerManager": true,
                "networkPlugin": "azure",
                "containerRuntime": "containerd",
                "addons": [
                    {
                        "name": "azurelustre-csi-driver",
                        "enabled": false
                    },
                    {
                        "name": "tiller",
                        "enabled": true
                    }
                ]
            }
        },
        "masterProfile": {
            "count": 1,
            "dnsPrefix": "{dnsPrefix}",
            "vmSize": "Standard_DS2_v2"
        },
        "agentPoolProfiles": [
            {
                "name": "agentpool1",
                "count": 2,
                "vmSize": "Standard_DS2_v2",
                "availabilityProfile": "AvailabilitySet",
                "storageProfile": "ManagedDisks",
                "distro": "aks-ubuntu-18.04"
            }
        ],
        "linuxProfile": {
            "adminUsername": "azureuser",
            "ssh": {
                "publicKeys": [
                    {
                        "keyData": "{keyData}"
                    }
                ]
            }
        },
        "servicePrincipalProfile": {
            "clientID": "{clientID}",
            "secret": "{secret}"
        }
    }
}
